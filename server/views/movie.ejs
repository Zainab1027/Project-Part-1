<%- include Partials/header.ejs %>
<%- include Partials/main_nav.ejs %>
<main class="container">
    <div class="row">
        <div class="offset-md-1 col-md-10">
            <h1><%= title %></h1>
        <table class="table table-bordered table-striped table-hover">
            <!--Table Header-->
            <tr class="d-flex">
                <th class="text-center col-2">Title</th>
                <th class="text-center col-2">Genre</th>
                <th class="text-center col-2">Year</th>
                <th class="col-5">Description</th>
            </tr>
            <% for(let count=0;count<BookList.length; count++) { %>
            <tr class="d-flex">
                <td class="text-center col-2"><%= MoviesList[count].Title %></td>
                <td class="text-center col-2"><%= MoviesList[count].Genre %></td>
                <td class="text-center col-2"><%= MoviesList[count].Year %></td>
                <td class="col-5"><%= MoviesList[count].Description %></td>
            </tr>
            <% } %>
        </table>
        </div>
    </div>
</main> 
<%- include Partials/footer.ejs %>